  <select id="find" parameterType="java.util.HashMap" resultMap="sh">
	  		SELECT * FROM pricehistory JOIN
		user ON  user.`uid` = pricehistory.`uid` 
		JOIN goods ON goods.`gid` = pricehistory.`gid`
	
		<where>
			<if test="uid != null">
				and pricehistory.`uid` = #{uid}
			</if>
			<if test="gid != null">
				and pricehistory.`gid` = #{gid}
			</if>
			<if test="phid != null">
				and phid = #{phid}
			</if>
			<if test ="search !=null">
				user.uname like '%${search}%' or goods.gname like '%${search}%'
			</if>
		</where>
		 	
		 	limit #{page.startLine} , #{page.size}
  
  </select>
  
